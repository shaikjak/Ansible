- name: Multitask play-1
  hosts: all
  become: yes
  tasks:
    - name: Play-1
      package: ansible.builtin.debug
      msg: "Hello I am from PLAY-1 and TASK-1"

- name: Multitask play-2
  hosts: all
  become: yes
  tasks:
    - name: Play-1
      package: ansible.builtin.dnf
      status: start
      enabled: true